      "autoprefixer": "10.4.16",
      "eslint": "8.50.0",
      "eslint-config-next": "13.5.3",
      "eslint-config-prettier": "9.0.0",
      "husky": "8.0.3",
      "lint-staged": "14.0.1",
      "postcss": "8.4.31",
      "prettier": "3.0.3",
      "prettier-plugin-tailwindcss": "0.5.5",
