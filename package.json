      "@typescript-eslint/parser": "6.16.0",
      "autoprefixer": "10.4.16",
      "eslint": "8.56.0",
      "eslint-config-next": "14.0.4",
      "eslint-config-prettier": "9.1.0",
      "husky": "8.0.3",
      "lint-staged": "15.2.0",
      "postcss": "8.4.33",
      "prettier": "3.1.1",
      "prettier-plugin-tailwindcss": "0.5.10",
