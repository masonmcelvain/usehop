      "@typescript-eslint/parser": "5.60.0",
      "autoprefixer": "10.4.14",
      "eslint": "8.43.0",
      "eslint-config-next": "13.4.7",
      "eslint-config-prettier": "8.8.0",
      "husky": "8.0.3",
      "lint-staged": "13.2.2",
      "postcss": "8.4.24",
      "prettier": "2.8.8",
      "prettier-plugin-tailwindcss": "0.3.0",
