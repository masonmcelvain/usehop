      "@typescript-eslint/parser": "6.2.1",
      "autoprefixer": "10.4.14",
      "eslint": "8.46.0",
      "eslint-config-next": "13.4.12",
      "eslint-config-prettier": "8.10.0",
      "husky": "8.0.3",
      "lint-staged": "13.2.3",
      "postcss": "8.4.27",
      "prettier": "2.8.8",
      "prettier-plugin-tailwindcss": "0.4.1",
