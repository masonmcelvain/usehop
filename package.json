   "license": "MIT",
   "description": "Website to showcase the Hop browser extension",
   "author": "Mason McElvain",
   "repository": {
      "type": "git",
      "url": "https://github.com/masonmcelvain/usehop"
   },
   "packageManager": "pnpm@9.13.2",
