      "@typescript-eslint/parser": "7.18.0",
      "autoprefixer": "10.4.20",
      "eslint": "8.57.1",
      "eslint-config-next": "15.0.4",
      "eslint-config-prettier": "9.1.0",
      "husky": "9.1.7",
      "lint-staged": "15.2.11",
      "postcss": "8.4.49",
      "prettier": "3.4.2",
