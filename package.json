      "@typescript-eslint/parser": "7.18.0",
      "autoprefixer": "10.4.20",
      "eslint": "8.57.1",
      "eslint-config-next": "14.2.18",
      "eslint-config-prettier": "9.1.0",
      "husky": "9.1.7",
      "lint-staged": "15.2.10",
      "postcss": "8.4.49",
      "prettier": "3.3.3",
      "prettier-plugin-tailwindcss": "0.6.8",
