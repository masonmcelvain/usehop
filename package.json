      "@typescript-eslint/parser": "7.16.1",
      "autoprefixer": "10.4.19",
      "eslint": "8.57.0",
      "eslint-config-next": "14.2.5",
      "eslint-config-prettier": "9.1.0",
      "husky": "9.0.11",
      "lint-staged": "15.2.7",
      "postcss": "8.4.39",
      "prettier": "3.3.3",
      "prettier-plugin-tailwindcss": "0.6.5",
