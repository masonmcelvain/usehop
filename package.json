      "@typescript-eslint/parser": "7.1.0",
      "autoprefixer": "10.4.18",
      "eslint": "8.57.0",
      "eslint-config-next": "14.1.3",
      "eslint-config-prettier": "9.1.0",
      "husky": "9.0.11",
      "lint-staged": "15.2.2",
      "postcss": "8.4.35",
      "prettier": "3.2.5",
      "prettier-plugin-tailwindcss": "0.5.12",
